{
  "name": "@rackai/domql",
  "description": "DOM rendering Javascript framework at early stage.",
  "private": false,
  "author": "rackai",
  "version": "0.0.13",
  "repository": "https://github.com/rackai/domql",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "parcel example/index.html --no-cache",
    "build": "parcel build example/index.html",
    "standard": "npx standard \"src/**/*.js\"",
    "test": "yarn standard && jest --coverage --coverageReporters=text-lcov | coveralls",
    "test-watch": "jest --watch"
  },
  "source": true,
  "dependencies": {
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.1.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.5",
    "eslint": "^6.1.0",
    "eslint-plugin-jest": "^22.14.1",
    "jest": "^26.1.0",
    "nodemon": "^1.19.1",
    "parcel-bundler": "^1.12.4",
    "standard": "^13.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "browserslist": [
    "> 1%",
    "ie >= 9"
  ]
}
